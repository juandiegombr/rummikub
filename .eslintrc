{
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "react-app",
    "react-app/jest"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["warn", {}, {"usePrettierrc": true}]
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
